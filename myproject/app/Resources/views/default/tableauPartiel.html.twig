            <td>{{ cafeUnique.adresse }}</td>
            <td>{{ cafeUnique.arrondissement }}</td>
            <td>{{ cafeUnique.geoLatitude }}</td>
            <td>{{ cafeUnique.prixComptoir }}</td>
            <td>{{ cafeUnique.prixSalle }}</td>
