<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Commentaires" table="commentaires">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idCafe" type="integer" column="id_cafe" nullable="false"/>
    <field name="date" type="string" column="date" length="250" nullable="false"/>
    <field name="texte" type="string" column="texte" length="535" nullable="false"/>
    <field name="etoile" type="integer" column="etoile" nullable="false"/>
    <field name="auteur" type="string" column="auteur" length="65" nullable="false"/>
  </entity>
</doctrine-mapping>
